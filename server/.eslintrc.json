{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": "error",
        "no-console": "off",
        "implicit-arrow-linebreak": "off",
        "consistent-return": "off",
        "no-underscore-dangle": "off",
        "comma-dangle": "off",
        "operator-linebreak": "off",
        "no-empty": "off"
    }
}
